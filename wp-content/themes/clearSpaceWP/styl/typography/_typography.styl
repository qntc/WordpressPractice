// plantin Italic
@font-face {
	font-family: 'plantinItalic';
	src: url('assets/fonts/plantinItalic.eot');
	src: local('☺'), url('assets/fonts/plantinItalic.woff') format('woff'), url('assets/fonts/plantinItalic.ttf') format('truetype'), url('assets/fonts/plantinItalic.svg') format('svg');
	font-weight: normal;
	font-style: normal;
}


